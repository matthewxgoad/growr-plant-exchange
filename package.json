    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.13.2",
    "mongoose-unique-validator": "^2.0.3",
